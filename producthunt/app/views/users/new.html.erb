<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user, url: users_path) do |f| %>
      <%= f.label 'Username' %>
      <%= f.text_field :username %>

      <%= f.label 'Email' %>
      <%= f.email_field :email %>

      <%= f.label 'Password' %>
      <%= f.password_field :password %>

      <%= f.label 'Credntials' %>
      <%= f.text_field :credentials %>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>